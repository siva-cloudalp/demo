			<% if (email_change_verification) { %>
                email_change_verification = '<%= email_change_verification %>';
            <% } %>

			var translationUrl = '<%= hasTranslations ? session.getAbsoluteUrl2(app_includes.myaccount.translations[Language]) : '' %>';
